#==========================================================================
# Start Section: General Utility Functions
#==========================================================================
function setGnomeTerminalTitle() {
local NEW_TITLE="$1";
PS1="[e]0;${NEW_TITLE}a]${debian_chroot:+($debian_chroot)}[033[01;32m]u@h[033[00m] [033[01;34m]w $[033[00m]";
}
function makeThenChangeDir() {
local NEW_DIR="$1";
mkdir -p "${NEW_DIR}";
cd "${NEW_DIR}";
}
